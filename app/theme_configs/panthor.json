{
	"url": "git@github.com:voceconnect/panthor.git",
	"replacements": [
		{"find": "Text Domain: panthor", "replace" : "Text Domain: <%= themeTextDomain %>"},
		{"find": "'panthor'", "replace" : "'<%= themeTextDomain %>'"},
		{"find": "panthor-", "replace" : "<%= themeSlug %>"},
		{"find": "panthor_", "replace" : "<%= themeUnderScored %>_"},
		{"find": "([\\W])panthor", "replace" : "$1<%= themeSlug %>"},
		{"find": "([\\W])Panthor", "replace" : "$1<%= themeName %>"}
	]
}