{
	"url": "https://github.com/voceconnect/skeletor/archive/master.tar.gz",
	"replacements": [
		{"find": "Text Domain: skeletor", "replace" : "Text Domain: <%= themeTextDomain %>"},
		{"find": "'skeletor'", "replace" : "'<%= themeTextDomain %>'"},
		{"find": "skeletor-", "replace" : "<%= themeSlug %>"},
		{"find": "skeletor_", "replace" : "<%= themeUnderScored %>_"},
		{"find": "([\\W])skeletor", "replace" : "$1<%= themeSlug %>"},
		{"find": "([\\W])Skeletor", "replace" : "$1<%= themeName %>"}
	]
}